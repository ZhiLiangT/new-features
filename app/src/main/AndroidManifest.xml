<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="omeng.bbwhm.com.weixin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui_characteristic.ClipppingActivity"
            android:label="@string/title_activity_clippping"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui_characteristic.RippleActivity"
            android:label="@string/title_activity_ripple"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ThreadActivity" />
        <activity android:name=".event_distribute.EventDistributeActivity" />
        <activity android:name=".custom_view.CustomActivity" />
        <activity android:name=".custom_view.ac.ScrollTopActivity"></activity>
    </application>

</manifest>