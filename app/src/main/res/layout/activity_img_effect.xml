        android:layout_height="150dp" />
    <ImageView
        android:id="@+id/img_3"
        android:src="@mipmap/link"
        android:layout_marginTop="10dp"
        android:scaleType="fitXY"
        android:layout_width="200dp"
        android:layout_height="150dp" />
</LinearLayout>
